///r44_zero()
{
  var M;
  M[0, 0] = 0;
  M[0, 1] = 0;
  M[0, 2] = 0;
  M[0, 3] = 0;
  M[1, 0] = 0;
  M[1, 1] = 0;
  M[1, 2] = 0;
  M[1, 3] = 0;
  M[2, 0] = 0;
  M[2, 1] = 0;
  M[2, 2] = 0;
  M[2, 3] = 0;
  M[3, 0] = 0;
  M[3, 1] = 0;
  M[3, 2] = 0;
  M[3, 3] = 0;
  return M;
}
